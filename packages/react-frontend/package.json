{
  "private": true,
  "name": "react-frontend",
  "description": "Frontend (React) sources for the CS Toolkit",
  "author": "ZechyW",
  "version": "0.1.0",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "dev": "cross-env BROWSER=browser.js react-scripts start",
    "eject": "react-scripts eject",
    "profile": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "test": "react-scripts test --runInBand --transformIgnorePatterns \"node_modules/(?!(lodash-es))/\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "ag-grid-community": "^20.1.0",
    "ag-grid-react": "^20.1.0",
    "axios": "^0.18.0",
    "bulma": "^0.7.4",
    "bulma-helpers": "^0.3.6",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "date-fns": "^2.0.0-alpha.27",
    "lodash-es": "^4.17.11",
    "minimalistic-assert": "^1.0.1",
    "node-sass": "^4.11.0",
    "noty": "^3.2.0-beta",
    "pako": "^1.0.8",
    "prop-types": "^15.7.2",
    "raf-schd": "^4.0.0",
    "react": "^16.8.1",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^3.0.2",
    "react-dom": "^16.8.1",
    "react-grid-layout": "^0.16.6",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.5",
    "react-spring": "^8.0.7",
    "react-tag-input": "^6.3.0",
    "reconnecting-websocket": "^4.1.10",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.1",
    "redux-starter-kit": "^0.4.3",
    "redux-thunk": "^2.3.0",
    "redux-websocket-bridge": "^0.3.0-0",
    "selectorator": "^4.0.3",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "flow-bin": "^0.94.0",
    "opn": "^5.4.0",
    "webpack-bundle-analyzer": "^3.0.4"
