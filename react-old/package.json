{
  "name": "cs-toolkit",
  "version": "0.1.0",
  "description": "(Roughly) Minimalist framework for exploring code switching data",
  "main": "frontend/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch --info-verbosity verbose",
    "profile": "webpack --profile --json > webpack-profile.json && webpack-bundle-analyzer webpack-profile.json ./react/built",
    "build-docs": "cd docs && poetry run make html",
    "watch-docs": "cd docs && chokidar \"**/*.rst\" \"../README.md\" -c \"poetry run make html\" --initial",
    "collectstatic": "cd django && poetry run python manage.py collectstatic --clear --noinput",
    "stage": "npm run build && npm run build-docs && npm run collectstatic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZechyW/cs-toolkit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZechyW/cs-toolkit/issues"
  },
  "homepage": "https://github.com/ZechyW/cs-toolkit#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bulma": "^0.7.2",
    "bulma-helpers": "^0.3.6",
    "chokidar-cli": "^1.2.1",
    "css-loader": "^2.1.0",
    "datatables-bulma": "^1.0.1",
    "datatables.net": "^1.10.19",
    "date-fns": "^2.0.0-alpha.27",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "gfynonce": "^1.0.1",
    "howler": "^2.1.1",
    "husky": "^1.3.1",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "1.16.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^3.0.2",
    "react-dom": "^16.7.0",
    "react-draggable": "^3.1.1",
    "react-grid-layout": "^0.16.6",
    "react-resize-detector": "^3.4.0",
    "react-tag-input": "^6.2.1",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "weak-key": "^1.0.1",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run stage && git diff --quiet && git diff --staged --quiet || git commit -am \"Static: Auto-committing built static files.\""
    }
  }
}
