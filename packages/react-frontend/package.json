{
  "private": true,
  "name": "react-frontend",
  "description": "Frontend (React) sources for the CS Toolkit",
  "author": "ZechyW",
  "version": "0.1.0",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "dev": "cross-env BROWSER=browser.js DJANGO_PORT=${DJANGO_PORT:8080} react-scripts start",
    "eject": "react-scripts eject",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(lodash-es))/\""
  },
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@naholyr/cross-env": "^1.0.0",
    "bulma": "^0.7.4",
    "bulma-helpers": "^0.3.6",
    "lodash-es": "^4.17.11",
    "node-sass": "^4.11.0",
    "opn": "^5.4.0",
    "prop-types": "^15.7.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^3.0.2",
    "react-redux": "^6.0.0",
    "react-tag-input": "^6.2.1",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-starter-kit": "^0.4.3",
    "selectorator": "^4.0.3",
    "source-map-explorer": "^1.6.0",
    "webpack-bundle-analyzer": "^3.0.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "babel-preset-react-app": "7.0.0"
  }
}
